{
  "profiles": {
    "http": {
      "commandName": "Project",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "publishAllPorts": true,
      "useSSL": true,
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:5179",
      "launchUrl": "http://localhost:5179/scalar/v1",
      "launchBrowser": true
    },
    "https": {
      "commandName": "Project",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:7079;http://localhost:5179",
      "launchUrl": "https://localhost:7079/scalar/v1",
      "launchBrowser": true
    },
    "Container (Dockerfile)": {
      "commandName": "Docker",
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
      "environmentVariables": {
        "ASPNETCORE_HTTPS_PORTS": "8081",
        "ASPNETCORE_HTTP_PORTS": "8080",
        "RABBITMQ_HOST": "host.docker.internal",
        "RabbitMqOptions__HostName": "host.docker.internal",
        "RabbitMqOptions__Port": "5672",
        "RabbitMqOptions__UserName": "guest",
        "RabbitMqOptions__Password": "guest"
      },
      "publishAllPorts": true,
      "useSSL": true
    }
  },
  "$schema": "https://json.schemastore.org/launchsettings.json"
}